@article{nicholson_will_2023,
	title = {Will {It} {Ever} {Be} {FAIR}?: {Making} {Archaeological} {Data} {Findable}, {Accessible}, {Interoperable}, and {Reusable}},
	volume = {11},
	issn = {2326-3768},
	shorttitle = {Will {It} {Ever} {Be} {FAIR}?},
	url = {https://www.cambridge.org/core/journals/advances-in-archaeological-practice/article/will-it-ever-be-fair/9073D28A74C2FE87E53B3C5A7437AC74},
	doi = {10.1017/aap.2022.40},
	abstract = {A fundamental task of archaeology is to address challenging scientific questions related to the complexity of human societies. If we are to systematically understand the processes that affect human societies on multiple spatial and temporal scales, research leveraging existing archaeological data is essential. However, only a fraction of the data from archaeological projects are publicly findable or accessible, let alone interoperable or reusable. This is the case despite statements of disciplinary ethics, availability of capable technologies for data stewardship, publications providing guidance, and legal mandates. This article introduces the FAIR principles for data stewardship in North American archaeology, which state that data should be Findable, Accessible, Interoperable, and Reusable. We call for efforts to promote widespread adoption of the FAIR and CARE (Collective benefit, Authority to control, Responsibility, and Ethics) principles among professional organizations, publishers, data repositories, and researchers. We also call for adoption and implementation of requirements to adhere to these principles by governmental agencies, funding bodies, and other regulators of archaeological research. Ultimately, adoption of the FAIR principles in an ethical framework contributes to our understanding of our human experience and can lead to greater integration and reuse of research results, fostering increased partnerships between academia and industry., Una tarea fundamental de la arqueolog√≠a es abordar preguntas cient√≠ficas desafiantes relacionadas con la complejidad de las sociedades humanas. Si queremos comprender sistem√°ticamente los procesos que afectan a las sociedades humanas en m√∫ltiples escalas espaciales y temporales, investigaciones que hagan uso de datos arqueol√≥gicos existentes es esencial. Sin embargo, s√≥lo una fracci√≥n de los datos de los proyectos arqueol√≥gicos se pueden encontrar o son p√∫blicamente accesibles, sin importar que sean interoperables o reutilizables. Este es el caso a pesar de las declaraciones de √©tica disciplinaria, la disponibilidad de tecnolog√≠as capaces para la administraci√≥n de datos, publicaciones que brindan orientaci√≥n y mandatos legales. Este art√≠culo presenta los principios FAIR para la administraci√≥n de datos en la arqueolog√≠a de Am√©rica del Norte, los cuales establecen que los datos deben ser localizables, accesibles, interoperables y reutilizables. Pedimos mayor esfuerzo para promover la adopci√≥n generalizada de los principios FAIR y CARE (beneficio colectivo, autoridad para controlar, responsabilidad y √©tica) entre organizaciones profesionales, editores, repositorios de datos e investigadores. Tambi√©n hacemos un llamado a la adopci√≥n e implementaci√≥n de requisitos para adherirse a estos principios por parte de agencias gubernamentales, organismos de financiaci√≥n y otros reguladores de la investigaci√≥n arqueol√≥gica. En √∫ltima instancia, la adopci√≥n de los principios FAIR en un marco √©tico contribuye a nuestra comprensi√≥n de nuestra experiencia humana y puede conducir a una mayor integraci√≥n y reutilizaci√≥n de los resultados de la investigaci√≥n, fomentando una mayor asociaci√≥n entre la academia y la industria.},
	language = {en},
	number = {1},
	urldate = {2024-10-14},
	journal = {Advances in Archaeological Practice},
	author = {Nicholson, Christopher and Kansa, Sarah and Gupta, Neha and Fernandez, Rachel},
	month = feb,
	year = {2023},
	keywords = {accesible, accessible, CARE, ciclo de vida de los datos, data life cycle, encontrable, findable, interoperable, interoperables, metadata, metadatos, reusable, reutilizable},
	pages = {63--75},
}

